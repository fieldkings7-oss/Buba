; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go in here are just too many to be
; handled manually.
;
; You can take your changes to the git repo.

config_version=5

[application]

config/name="Plants vs Space Zombies"
config/description="A Plants vs Zombies style game with space theme. Early prototype."
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.0", "forward_plus")
config/icon="res://assets/icon.png"

[autoload]

GameManager="*res://scripts/game_manager.gd"
SaveManager="*res://scripts/save_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[rendering]

textures/canvas_textures/default_texture_filter=0

[android]

orientation="landscape"
min_sdk_version=21
target_sdk_version=33
